<h1>Listing panels</h1>

<table>
  <tr>
    <th>Name</th>
    <th>App</th>
    <th>User</th>
    <th>Content</th>
    <th>Enabled</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @panels.each do |panel| %>
  <tr>
    <td><%= panel.name %></td>
    <td><%= panel.app_id %></td>
    <td><%= panel.user_id %></td>
    <td><%= panel.content %></td>
    <td><%= panel.enabled %></td>
    <td><%= link_to 'Show', panel %></td>
    <td><%= link_to 'Edit', edit_panel_path(panel) %></td>
    <td><%= link_to 'Destroy', panel, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Panel', new_panel_path %>
